<%= stylesheet_link_tag 'signin' %>

<div class="login main">
<p class='sign top'>Sign in</p>
<p class=sign>ESC Member Point Tracker </p>
  <%= form_tag(default_login_path, :method => :post, class: 'form1') do %>
      <td><%= text_field_tag(:username, params[:username], placeholder: 'username') %></td>
      <td><%= password_field_tag(:password, '', placeholder: 'password') %></td>
      <td><%= submit_tag("Log In", class: 'submit center') %></td>
  <% end %>
  <%# <td><%= link_to('Sign in with Google', google_login_path, class: 'center') %> <%#</td> %>
  <br>
  <td><%= link_to('Register', register_path, class: 'submit submit_register') %></td>
</div>

